<!DOCTYPE html>
<html lang="en">
<head>
    <%- include ('../partials/head') %>
 <title><%= title  %></title>
</head>
<body>
    <header>
        <%- include ('../partials/header') %>
    </header>
    <main>
        <section class="seccion-uno">


            <ul>
                <% errors.forEach(elemento=>{ %>

                   <li> <%= elemento.msg%></li>

                    <%});%>
            </ul>

            <form action="/signupmarca" method="post" class="on-page-marca-registracion">
                <!-- Primero algunos datos.. despues continua -->
                <label for="title">Nombre comercial</label>
                <input type="text" name="title" id="title">
                <label for="">Telefono de contacto</label>
                <input type="number" name="" id="">
                <label for="email">Email</label>
                <input type="email" name="email" id="email">

                <label for="">Razon social</label>
                <input type="text" name="" id="">
                <label for="">Fecha de constitucion</label>
                <input type="date" name="" id="" min="1800" max="today">
                <label for="">Domicilio social</label>
                <input type="text" name="" id="">
                <label for="">Codigo Postal</label>
                <input type="tel" name="" id="">
                <label for="">Localidad</label>
                <input type="tel" name="" id="">
                <label for="">Provincia</label>
                <input type="tel" name="" id="">
                <label for="">Pais</label>
                <input type="tel" name="" id="">

                <input type="submit" value="Enviar">
            </form>
        </section>
    </main>
    <footer>
        <%- include ('../partials/footer')%>
    </footer>
</body>
</html>